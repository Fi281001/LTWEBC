@model _19T1021292.DomainModels.Shipper
@{

}

<div class="box box-primary">

    <div class="box-body">
        <form action="~/Shipper/Save" method="post">
            @Html.AntiForgeryToken()
            <input type="hidden" name="ShipperID" value="@Model.ShipperID" />
            <div class="form-group">
                <label>Tên shipper:</label>
                <input type="text" name="ShipperName" value="@Model.ShipperName" class="form-control">
            </div>
            <div class="form-group">
                <label>Số điện thoại:</label>
                <input type="text" name="Phone" value="@Model.Phone" class="form-control">
            </div>

            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-primary">
                        <i class="glyphicon glyphicon-floppy-disk"></i>
                        Lưu dữ liệu
                    </button>
                    <a href="~/Shipper" class="btn btn-warning">Quay lại</a>
                </div>
            </div>

        </form>
    </div>

</div>

